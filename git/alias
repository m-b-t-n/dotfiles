	dfc = diff --cached
	dfq = diff --quiet
	dfn = diff --name-only
	dt = difftool
	cur-br = rev-parse --abbrev-ref @
	cpk = cherry-pick
	hrst = reset --hard
	srst = reset --soft
	rst = reset
	rb = rebase
	ri = rebase -i
